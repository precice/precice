#---------------------------------------------------------------------------
# Configuration options related to the HTML layout
#---------------------------------------------------------------------------
NAVIGATION_CHAR_LENGTH              = 30
LIMIT_COMPACT_IN_OVERVIEW_TABLES    = 100
